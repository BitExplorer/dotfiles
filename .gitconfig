[user]
  email = henninb@msn.com
  name = Brian Henning
[alias]
  co = checkout
  ci = commit
  st = status -sb
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  cob = checkout -b
  pom = push origin master
  cm = commit -m
  plom = pull origin master
  f = "!git ls-files | grep -i"
  aa = add -A .
  unstage = reset HEAD --
[push]
  default = simple
[merge]
  tool = vimdiff
[diff]
#  tool = vimdiff
  external = git-diff-wrapper
[core]
  pager = less -F -X
  editor = nvim
[color]
  diff = auto
  status = auto
  branch = auto
  interactive = auto
  ui = true
  pager = true
# https://github.com/jchnkl/etc.irssi/blob/2a5e44fc297c45c594a39f94404dbff0b8a2e424/.irssi/readme.md
[filter "irssi-pw"]
  clean = "sed -e 's/sasl_password = .*;/sasl_password \"********\";/'"
[filter "ks-pw"]
  clean = "sed -e 's/KEYSTORE_PASSWORD=\".*\"/KEYSTORE_PASSWORD=\"********\"/'"
[filter "influxdb-pw"]
  clean = "sed -e 's/INFLUXDB_PASSWORD=\".*\"/INFLUXDB_PASSWORD=\"********\"/'"
[filter "activemq-pw"]
  clean = "sed -e 's/ACTIVEMQ_PASSWORD=\".*\"/ACTIVEMQ_PASSWORD=\"********\"/'"
[filter "chef-pw"]
  clean = "sed -e 's/CHEF_PASSWORD=\".*\"/CHEF_PASSWORD=\"********\"/'"
[filter "ssmtp-pw"]
  clean = "sed -e 's/SSMTP_PASSWORD=\".*\"/SSMTP_PASSWORD=\"********\"/'"
[filter "postfix-pw"]
  clean = "sed -e 's/POSTFIX_PASSWORD=\".*\"/POSTFIX_PASSWORD=\"********\"/'"
