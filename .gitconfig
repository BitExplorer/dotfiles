#	fetch = +refs/heads/*:refs/remotes/origin/*
# [branch "master"]
# 	remote = origin
# 	merge = refs/heads/master
[merge]
  tool = vimdiff
[diff]
  tool = vimdiff
[core]
  pager = less -F -X
