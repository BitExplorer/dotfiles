[user]
  email = henninb@msn.com
  name = Brian Henning
[alias]
  co = checkout
  ci = commit
  st = status -sb
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  cob = checkout -b
  pom = push origin master
[push]
  default = simple
[merge]
  tool = vimdiff
[diff]
#  tool = vimdiff
  external = git-diff-wrapper
[core]
  pager = less -F -X
